extends layout

block content
    .container-fluid
        include includes/layout_nav.pug
        .row
            .col-xs-1.col-sm-1.col-md-1.col-lg-1
            .col-xs-10.col-sm-10.col-md-10.col-lg-10
                div(role='tabpanel')
                    ul.nav.nav-tabs(role='tablist')
                        li.active(role='presentation')
                            a(href='#home', aria-controls='home', role='tab', data-toggle='tab') Session Management
                        li(role='presentation')
                            a#uploadToMaster(href='#tab', aria-controls='tab', role='tab', data-toggle='tab') Upload To Master Server
                    .tab-content
                        .tab-pane.active#home(role='tabpanel')
                            button#clearAllRecores.btn.btn-warning.btn-sm(style='margin-top:25px;margin-left:25px;') Start New Session
                        .tab-pane#tab(role='tabpanel') 
                            h4.text-success
                                caption 
                                    strong Completed Exams
                            table.table.table-striped.table-bordered.table-hover
                                thead
                                    tr
                                        th(width='5%') Sr. No
                                        th(width='20%') Exam Name
                                        th(width='10%') Duration
                                        th(width='15%') Batch No
                                        th(width='30%') Action
                                tbody#done_exams_list_tbody
                                    tr
                                        td(colspan="10", style="text-align:center") No Exams Found
                
block inline_css
    style.
        .nav-li{
            color:red
        } 
block css_link
    link(rel='stylesheet', href="/plugins/datatable/datatables.min.css")
block js_link
    script(src="/plugins/datatable/datatables.min.js")
    script(src="/js/adminner.js")
block inline_js
    script.
        $(document).ready(function() {
            $('.adminner-active').addClass('active');
        });
    

       
        