    script(type='text/javascript').
        var lang = 'eng';
        function timeOut() {
            setTimeout(function () {
                $('.c-alert-msg').slideUp(750);
            }, 3000);
        }

        timeOut();
        function hideMe(_this, event) {
            if ($('#is_agree').prop('checked') == false) {
                $('#is_agree').siblings('strong').css('color', 'red');
                $('#is_agree').siblings('strong').hide(200).show(200);
                event.preventDefault();
                return false;
            } else {
                $(_this).addClass('hide');
                $('.c_approveing').removeClass('hide');
            }
        }
        $('#is_agree').on('click', function (e) {
        $('#is_agree').siblings('strong').css('color', 'black');
        });
        function setAgreementTestLang(_this,event){
            if($(_this).val() == 'eng') 
            {   
                $('#imp_note').html('Note: In case of difficulty due to technical reasons before   or after the examination is started, the candidate will be given the next time / date.');
                $('#marathi').addClass('hide');
                $('#english').removeClass('hide');
            }else{
                $('#imp_note').html('नोट : परिक्षा सुरू झाल्यानंतर किंवा अगोदर काही तांत्रिक कारणामुळे अडचन आल्यास उमेदवारास पुढील वेळ/तारीख देण्यात येईल.');
                $('#marathi').removeClass('hide');
                $('#english').addClass('hide');
            }
        }